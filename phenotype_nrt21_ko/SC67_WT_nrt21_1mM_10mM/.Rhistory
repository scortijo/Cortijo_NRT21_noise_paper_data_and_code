knitr::opts_chunk$set(echo = TRUE)
library(tidyverse)
library(ggpubr)
library(RColorBrewer)
SC67 <- read_tsv("SC67_root_measure.txt")
View(SC67)
SC67
SC67 <- read_tsv("SC67_root_measure.txt") %>%
mutate(root_length_mm=length_px/scale_px_mm)
SC67
ggplot(SC67, aes(x=KNO3, y=root_length_mm, col=genotype))+
geom_boxplot() +
stat_compare_means(label.x=1.25, label.y=0.55, size=4.5) +
ylab("Primary root length (mm)") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
