knitr::opts_chunk$set(echo = TRUE)
library(tidyverse)
library(ggpubr)
library(RColorBrewer)
SC68 <- read_tsv("SC68_root_measurement.txt") %>%
mutate(root_length_mm=length_px/scale_px_mm,
KNO3= factor(KNO3, levels=c("1mM", "10mM")),
LRD=lateralRoots/root_length_mm)
ggplot(SC68, aes(x=KNO3, y=root_length_mm, col=genotype))+
geom_boxplot() +
stat_compare_means(size=4.5, label="p.format") +
ylab("Primary root length (mm)") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
ggplot(SC687, aes(x=KNO3, y=LRD, col=genotype))+
geom_boxplot() +
stat_compare_means(size=4.5, label="p.format") +
ylab("Lateral root density") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
ggplot(SC68, aes(x=KNO3, y=LRD, col=genotype))+
geom_boxplot() +
stat_compare_means(size=4.5, label="p.format") +
ylab("Lateral root density") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
group_by(SC68, genotype, KNO3) %>%
summarise(CV=sd(root_length_mm, na.rm=TRUE)/mean(root_length_mm, na.rm = TRUE)) %>%
mutate(KNO3=factor(KNO3, levels=c("1mM", "10mM")))  %>%
ggplot(aes(x=KNO3, y=CV, col=genotype))+
geom_point() +
stat_compare_means(label="p.format", size=4.5) +
ylab("CV for primary root length") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
group_by(SC68, genotype, KNO3) %>%
summarise(CV=sd(LRD, na.rm=TRUE)/mean(LRD, na.rm = TRUE)) %>%
mutate(KNO3=factor(KNO3, levels=c("1mM", "10mM")))  %>%
ggplot(aes(x=KNO3, y=CV, col=genotype))+
geom_point() +
stat_compare_means(label="p.format", size=4.5) +
ylab("CV for primary root length") +
xlab(bquote('Concentration of KNO'[3]*' in media'))+
theme(legend.text=element_text(size=16),legend.title = element_text(size=16, face="bold"),
axis.text=element_text(size=16), axis.title=element_text(size=16),
plot.title = element_text(size=20,face="bold"),
panel.background = element_rect(fill = "white", colour = "grey50"))
