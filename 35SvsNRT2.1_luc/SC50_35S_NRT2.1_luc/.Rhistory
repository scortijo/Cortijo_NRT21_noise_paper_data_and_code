knitr::opts_chunk$set(echo = TRUE)
library(tidyverse)
library(ggpubr)
library(RColorBrewer)
SC50 <- read_tsv("SC50_35S_NRT21_luc_signal.txt")
View(SC50)
names(SC50)
SC50_norm <- mutate(SC50, RawIntDenDivArea=RawIntDen/area) %>%
filter(seedling=="background") %>%
group_by(genotype, plate) %>%
summarise(MeanBack=mean(RawIntDenDivArea)) %>%
full_join(SC50, by=c("genotype"="genotype", "plate"="plate")) %>%
mutate(RawIntDen_NormBack= RawIntDen-(MeanBack*area)) %>%
mutate(root_length_mm= length/4,
RawIntDen_NormBack_NormLen=RawIntDen_NormBack/root_length_mm) %>%
filter(seedling!="background")
View(SC50_norm)
names(SC50_norm)
ggplot(SC50_norm, aes(x=genotype, y=RawIntDen_NormBack_NormLen))+
geom_boxplot()
group_by(SC50_norm, genotype) %>%
summarise(cv=sd(RawIntDen_NormBack_NormLen)/mean(RawIntDen_NormBack_NormLen))
