scale_x_discrete(labels=c(expression(italic("pNRT2.1:LUC")),
expression(italic("arp6-1/pNRT2.1:LUC"))))
All_meanForNorm <- filter(arp6_8d, seedling!="background"  & Ninflux=="Yes") %>%
group_by(genotype, KNO3)%>%
summarise(average.RawIntDen_normBackg_normLen = mean(RawIntDen_normBackg_normLen, na.rm = TRUE),
n=n(),
CV=sd(RawIntDen_normBackg_normLen,
na.rm = TRUE)/average.RawIntDen_normBackg_normLen) %>%
mutate(n=paste("n =",n),
CV=paste("CV =", round(CV,2)))
All_MeanNormalised <- filter(arp6_8d, seedling!="background") %>%
full_join(., All_meanForNorm, by=c("genotype", "KNO3")) %>%
mutate(MeanNormalised_RawIntDen_normBackg_normLen=log2(RawIntDen_normBackg_normLen/average.RawIntDen_normBackg_normLen))
All_MeanNormalised
